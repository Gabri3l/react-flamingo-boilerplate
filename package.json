{
  "name": "react-firenze-boilerplate",
  "version": "1.0.0",
  "description": "A react boilerplate that gives more control to the developer",
  "main": "index.js",
  "author": "Gabriele Cimato <gabriele.cimato@gmail.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "build": "webpack --env.mode production --config ./config/webpack.base.js",
    "build:all": "npm run build && npm run build:server",
    "build:server": "webpack --env.mode server --config ./config/webpack.base.js",
    "dev": "webpack-dev-server --config ./config/webpack.base.js",
    "lint": "eslint --fix ./src/**/*.{js,jsx} ./src/**/**/*.{js,jsx} ./server/**/*.js",
    "prettier": "prettier --write ./src/**/*.{js,jsx} ./src/**/**/*.{js,jsx} ./server/**/*.js",
    "format": "npm run prettier && npm run lint",
    "postinstall": "npm run build:all",
    "start": "node dist/server/main.js",
    "stats": "webpack --env.mode production --profile --json > stats.json --config ./config/webpack.base.js",
    "test": "jest"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.53",
    "@babel/core": "^7.0.0-beta.53",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.53",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.42",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.53",
    "@babel/preset-env": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0-beta.53",
    "@babel/register": "^7.0.0-beta.53",
    "@react-firenze/react-loadable": "^5.3.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "clean-webpack-plugin": "^0.1.19",
    "compression": "^1.7.2",
    "dotenv": "^6.0.0",
    "emotion": "^9.2.5",
    "emotion-server": "^9.2.5",
    "eslint-plugin-react": "^7.10.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.10",
    "nyan-progress-webpack-plugin": "^1.2.0",
    "open-browser-webpack-plugin": "^0.0.5",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-emotion": "^9.2.6",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-svg-loader": "^2.1.0",
    "redux": "^4.0.0",
    "redux-slim-async": "^1.3.1",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "url-loader": "^0.5.9",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-merge": "^4.1.3",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "babel-jest": "^23.4.0",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "husky": "^1.0.0-rc.13",
    "jest": "^23.4.1",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "prettier-eslint": "^8.8.2",
    "react-test-renderer": "^16.4.1",
    "webpack-dev-server": "^3.1.4"
  }
}
